{
  "name": "smile-savers-dental",
  "version": "1.0.0",
  "description": "Premium dental practice website with HIPAA/ADA compliant mobile UI",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "eslint src --ext .ts,.tsx,.astro",
    "lint:fix": "eslint src --ext .ts,.tsx,.astro --fix",
    "format": "prettier --write \"src/**/*.{astro,ts,tsx,css,md}\"",
    "a11y": "pa11y-ci",
    "lighthouse": "lhci autorun"
  },
  "_comment_versions": "IMPORTANT: Always use @latest or check npm for current stable versions. The versions below are MINIMUM requirements - always upgrade to latest stable.",
  "dependencies": {
    "astro": "latest",
    "@astrojs/check": "latest",
    "@astrojs/tailwind": "latest",
    "tailwindcss": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-plugin-astro": "latest",
    "prettier": "latest",
    "prettier-plugin-astro": "latest",
    "prettier-plugin-tailwindcss": "latest"
  },
  "optionalDependencies": {
    "lucide-react": "latest",
    "@lhci/cli": "latest",
    "pa11y-ci": "latest"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "dental",
    "healthcare",
    "astro",
    "tailwindcss",
    "hipaa",
    "wcag",
    "accessibility"
  ],
  "author": "Smile Savers Dental",
  "license": "UNLICENSED",
  "private": true
}
